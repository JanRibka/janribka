{
  "name": "@janribka/components",
  "version": "0.0.2",
  "private": false,
  "author": "Jan Ribka",
  "description": "React components library for my projects",
  "main": "./src/index.ts",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./Button": "./dist/Button/index.js"
  },
  "keywords": [
    "react",
    "react-components",
    "library",
    "jr",
    "jan ribka",
    "radix-ui",
    "tailwind",
    "material design"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JanRibka/janribka",
    "directory": "packages/janribka-components"
  },
  "bugs": {
    "url": "https://github.com/JanRibka/janribka/issues"
  },
  "homepage": "https://github.com/JanRibka/janribka#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsup && pnpm build:exports && pnpm build:copy-files",
    "build:exports": "node ../../scripts/generate-exports.mjs",
    "build:copy-files": "node ../../scripts/copy-files.mjs",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "release": "pnpm build && pnpm version --no-git && pnpm publish --access public"
  },
  "dependencies": {
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}
